{
  "name": "ireporter-frontend",
  "version": "1.0.0",
  "description": "This is a user interface for pre-implemented API",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server --open --hot --port 3000 --mode development",
    "build": "webpack --mode production",
    "heroku-prebuild": "npm i",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rhytah/iReporter-frontend.git"
  },
  "keywords": [
    "ReactJs",
    "Flask"
  ],
  "author": "Rhytah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rhytah/iReporter-frontend/issues"
  },
  "homepage": "https://github.com/Rhytah/iReporter-frontend#readme",
  "dependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/setupTests.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.5",
    "css-loader": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-css-modules-transform": "^2.5.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "heroku-run-build-script": true
}
